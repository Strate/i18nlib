{
	"formats": {
		"default": "{streetAddress}\n{postalCode} {locality}\n{country}",
		"nocountry": "{streetAddress}\n{locality} {postalCode}"
	},
	"startAt": "end",
	"fields": [

		{
			"name": "postalCode",
			"line": "startAtLast",
			"pattern": "^[0-9٠١٢٣٤٥٦٧٨٩۴۵۶]{5}$",
			"matchGroup": 0
		},
	
		{
			"name": "locality",
			"line": "last",
			"pattern": "[A-zجثتبأرذدخحضصشسزفغعظطنملكقيوهـدُدِدَدٌدٍدً\\.\\-\\s']+",
			"matchGroup": 0
		}
		
	]
}
