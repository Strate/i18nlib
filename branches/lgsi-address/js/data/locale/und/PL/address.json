{
	"formats": {
		"default": "{streetAddress}\n{postalCode} {locality}\n{country}",
		"nocountry": "{streetAddress}\n{postalCode} {locality}"
	},
	
	"startAt": "end",
	"fields": [
		{ 
			"name": "locality",
			"pattern": "[AĄBCĆDEĘFGHIJKLŁMNŃOÓPRSŚTUWYZŹŻaąbcćdeęfghijklłmnńoóprsśtuwyzźż\\.\\-']+",
			"line": "last"
		},
		{
			"name": "postalCode",
			"line": "last",
			"pattern": "([0-9]{2}-[0-9]{3})"
		}
	]
}
