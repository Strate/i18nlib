		    },
			{
				"name": "postalCode",
				"line": "startAtLast",
				"pattern": "\\(?[0-9]{3}-[0-9]{3}\\)?$"
			},
			{
				"name": "locality",
				"line": "last",
				"pattern": "([0-9]{3}-[0-9]{3})?\\s*([A-zÀÁÈÉÌÍÒÓÙÚàáèéìíòóùúĀāĂăǍǎĒēĔĕĚěĪīĬĭǏǐŌōŎŏǑǒŪūŬŭǓǔ\\-']+)$",
