{
	"multiformat": true,
	"joinString": {
		"latin": ", ",
		"asian": " "
	},
	"formats": {
		"defaults": {
			"latin": "{streetAddress}\n{locality} {postalCode}\n{region}\n{country}",
			"asian": "{country}\n{region} {postalCode} {locality} {streetAddress}"
		},
		"nocountry": {
			"asian": "{region} {postalCode} {locality} {streetAddress}",
			"latin": "{streetAddress}\n{locality} {postalCode}\n{region}"
		}
	},
	"startAt": {
		"latin": "end",
		"asian": "beginning"
	},
	"fields": {
		"latin": [
		    {
		    	"name": "region",
		    	"line": "last",
