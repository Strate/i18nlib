			    ]
		    },
			{
				"name": "postalCode",
				"line": "startAtFirst",
				"pattern": "\\(?[0-9]{3}-[0-9]{3}\\)?"
			},
			{
				"name": "locality",
				"line": "startAtFirst",
				"pattern": "^[\\u1100-\\u11FF\\uAC00-\\uD7AF\\.\\-']+"
