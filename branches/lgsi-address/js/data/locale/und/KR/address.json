		    		"south jeolla do",
		    		"s. jeolla do",
		    		"s jeolla do",
		    		"north chungcheong",
		    		"n. chungcheong",
		    		"n chungcheong",
		    		"south chungcheong",
		    		"s. chungcheong",
		    		"s chungcheong",
