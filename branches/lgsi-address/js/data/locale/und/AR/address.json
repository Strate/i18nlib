{
    "formats": {
        "default": "{streetAddress}\n{postalCode} {locality}\n{country}",
        "nocountry": "{streetAddress}\n{postalCode} {locality}"
    },
    "startAt": "end",
    "fields": [
       
        {
            "name": "postalCode",
            "line": "startAtLast",
            "pattern": "[A-Za-z0-9]{8}"
        },
        {
            "name": "locality",
            "line": "last",
            "pattern": "[A-zÁÉÍÓÚáéíóú\\.\\-']+$"
        }
    ]
}
