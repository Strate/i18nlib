{
	"formats": {
		"default": "{streetAddress}\n{locality} {postalCode}\n{country}",
		"nocountry": "{streetAddress}\n{locality} {postalCode}"
	},
	"startAt": "end",
	"fields": [

		{
			"name": "postalCode",
			"line": "startAtLast",
			"pattern": "[0-9٠١٢٣٤٥٦٧٨٩۴۵۶]{5}( – [0-9١٢٣٤٥٦٧٨٩۴۵۶]{4})?$",
			"matchGroup": 0
		},

		{
			"name": "locality",
			"line": "last",
			"pattern": "[A-zجثتبأرذدخحضصشسزفغعظطنملكقيوهـدُدِدَدٌدٍدً\\.\\-\\s']+",
			"matchGroup": 0
		}
		
	]
}
