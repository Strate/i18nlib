{
    "formats": {
        "default": "{streetAddress}\n{postalCode} {locality}\n{country}",
        "nocountry": "{streetAddress}\n{postalCode} {locality}"
    },
    "startAt": "end",
    "fields": [
	{
            "name": "postalCode",
            "line": "startAtLast",
            "pattern": "[A-Z][0-9][0-9][0-9][0-9][A-Z][A-Z][A-Z]"
        },
	 {
            "name": "locality",
            "line": "startAtLast",
            "pattern": "[A-zÁÉÍÓÚáéíóú\\.\\-\\s{1}']+"
        }
    ]
}
