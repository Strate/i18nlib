{
	"formats": {
		"default": "{streetAddress}\n{locality} {postalCode}\n{country}",
		"nocountry": "{streetAddress}\n{locality} {postalCode}"
	},
	"startAt": "end",
	"fields": [
		{
			"name": "postalCode",
			"line": "last",
			"pattern": "([0-9٠١٢٣٤٥٦٧٨٩۴۵۶]{3,4})$",
			"matchGroup": 0
		},
		{
			"name": "locality",
			"line": "startAtLast",
			"pattern": "([A-zجثتبأرذدخحضصشسزفغعظطنملكقيوهـدُدِدَدٌدٍدً]+)$",
			"matchGroup": 0
		}
		
		
	]
}
